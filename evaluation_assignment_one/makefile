CC = gcc

moving_executable: moving.c 
	$(CC) -o moving_executable moving.c

find_divisor_executable: find_divisor.c
	$(CC) -o find_divisor_executable find_divisor.c

fibonacci_executable: fibonacci.c
	$(CC) -o fibonacci_executable fibonacci.c

powers_execuatble: powers.c
	$(CC) -o powers_execuatble powers.c
